<% if !@contacts.nil? %>
<table id="dynamic-table" class="table table-striped table-bordered table-hover">
  <thead>

  <tr>
    <th>Contact</th>
    <th>Date</th>
    <!--<th>Status</th>-->
  </tr>
  </thead>
  <tbody>
  <% @contacts.each do |c| %>
      <tr>
        <td><%= c.phone %></td>
        <td><%= c.created_at.strftime("%m/%d/%Y") %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<% else %>
<p> You currently do not have any contacts</p>
<% end %>