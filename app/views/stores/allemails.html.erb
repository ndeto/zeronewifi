<br><br>
<script>
  $(function () {
      $('#dynamic-table').DataTable();
      $("#download").click(function () {
          $("#dynamic-table").table2excel({
              exclude: ".noExl",
              name: "Excel Document Name",
              filename: "AllEmails" + new Date().toISOString().replace(/[\-\:\.]/g, "")+".xls",
              fileext: ".xls",
              exclude_img: true,
              exclude_links: true,
              exclude_inputs: true
          });
      });

  });
</script>

<div>
  <button class="my-button" id="download" style="margin: 1em;">Download</button>
  <br><br>
  <table id="dynamic-table" class="table table-striped table-bordered table-hover">
    <thead>
    <tr>
      <th>Email</th>
      <th>Date</th>
    </tr>
    </thead>
    <tbody>
    <% @contacts.each do |c| %>
        <tr>
          <td><%= c.email %></td>
          <td><%= c.created_at %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
<br><br>
</div>
